{
  "name": "@money-saver/backend",
  "version": "2.0.0",
  "description": "Personal expenses manager",
  "main": "daemon.js",
  "scripts": {
    "setup": "npm install && tsc --project tsconfig.json --traceResolution && uuidgen > ./target/memcached.key && uuidgen > ./target/session.key",
    "update": "npm install && tsc --project tsconfig.json --traceResolution",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "deploy:setup": "pm2 deploy production setup",
    "deploy:update": "pm2 deploy production update",
    "start": "node source/daemon.js -b finance.db -v",
    "test": "mocha --color --ui bdd Source/tests/unit/**/*test.js",
    "integration": "./Scripts/run-integration-tests.sh -s"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:idenisovs/money-saver.git"
  },
  "keywords": [
    "money",
    "finances",
    "cash",
    "expenses",
    "manager"
  ],
  "bugs": {
    "url": "https://github.com/idenisovs/money-saver/issues",
    "email": "i.denisovs@gmail.com"
  },
  "author": "Ilya Denisov",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "connect-memcached": "^1.0.0",
    "cookie-parser": "^1.4.5",
    "deferred": "^0.7.11",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "http-status": "^1.5.0",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "proxyquire": "^2.1.3",
    "request": "^2.88.2",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/memcached": "^2.2.6",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^17.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "sinon": "^11.1.1",
    "typescript": "^4.3.3"
  }
}
